{
  "name": "vecks",
  "version": "3.7.0",
  "description": "2D and 3D Vector Algebra library",
  "main": "lib/index.js",
  "scripts": {
    "lint": "standard",
    "unit": "mocha --compilers js:babel-register --recursive test/**",
    "unit:watch": "npm run unit -- --watch",
    "test": "npm run lint && npm run unit",
    "compile": "babel --optional runtime -d lib/ src/",
    "prepare": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:bjnortier/vecks.git"
  },
  "keywords": [
    "vector",
    "algebra"
  ],
  "author": "Ben Nortier <ben@bjnortier.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-plugin-transform-es2015-destructuring": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-register": "^6.22.0",
    "eslint": "^3.14.0",
    "expect": "^1.20.2",
    "mocha": "^3.2.0",
    "standard": "^10.0.2"
  },
  "dependencies": {},
  "standard": {
    "globals": [
      "describe",
      "it",
      "before",
      "beforeEach",
      "after"
    ],
    "ignore": [
      "lib/"
    ]
  }
}
