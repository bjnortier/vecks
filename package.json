{
  "name": "vecks",
  "version": "3.7.2",
  "description": "2D and 3D Vector Algebra library",
  "main": "lib/index.js",
  "scripts": {
    "lint": "standard",
    "unit": "mocha --require babel-core/register --recursive test/**",
    "unit:watch": "npm run unit -- --watch",
    "test": "npm run lint && npm run unit",
    "compile": "babel --optional runtime -d lib/ src/",
    "prepublishOnly": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:bjnortier/vecks.git"
  },
  "keywords": [
    "vector",
    "algebra"
  ],
  "author": "Ben Nortier <ben@bjnortier.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "expect": "^1.20.2",
    "mocha": "^5.2.0",
    "standard": "^10.0.3"
  },
  "dependencies": {},
  "standard": {
    "globals": [
      "describe",
      "it",
      "before",
      "beforeEach",
      "after"
    ],
    "ignore": [
      "lib/"
    ]
  }
}
